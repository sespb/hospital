<h1>Listing gerencias</h1>

<table>
  <tr>
    <th>Codigo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @gerencias.each do |gerencia| %>
  <tr>
    <td><%= gerencia.codigo %></td>
    <td><%= link_to 'Show', gerencia %></td>
    <td><%= link_to 'Edit', edit_gerencia_path(gerencia) %></td>
    <td><%= link_to 'Destroy', gerencia, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

